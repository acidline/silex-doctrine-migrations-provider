{
    "name": "kurl/silex-doctrine-migrations-provider",
    "description": "A doctrine migrations provider for silex",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^5.5|^7",
        "doctrine/dbal": "^2.2",
        "doctrine/migrations": "^1.0",
        "silex/silex": "^2.0",
        "symfony/console": "^2.3 | ^3.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.2",
        "phpunit/phpunit": "^3.7"
    },
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "autoload-dev": {
        "psr-0": { "": "tests/" }
    },
    "config": {
        "bin-dir": "bin"
    }
}
